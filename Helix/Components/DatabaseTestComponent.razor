@using Helix.Nodes
@using Helix.Types

@inject HelixDiagram Diagram

<MudCard Class="pa-8"
         @onkeydown:stopPropagation @onkeyup:stopPropagation
         @oncontextmenu:stopPropagation>
    <MudStack Row="true">
        <MudButton @onclick="Diagram.OnStore">Store Database</MudButton>
        <MudButton @onclick="Diagram.OnLoad">Load Database</MudButton>
    </MudStack>
</MudCard>

@code {

    [Parameter]
    [EditorRequired]
    public required DatabaseTestNode Node { get; set; }

    public void Qwe()
    {
        var f = (KeyboardEventArgs x) => { };
        Diagram.KeyDown -= f;
    }
}